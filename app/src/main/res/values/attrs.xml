<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="level" format="integer"/>
    <attr name="indecatorColor" format="color"/>


    <attr name="innerColor" format="color"/>

    <declare-styleable name="TribleSwitchButton">
        <attr name="innerColor"/>
    </declare-styleable>

    <declare-styleable name="FanBtn">
        <attr name="level"/>
        <attr name="indicatorColor"/>
        <attr name="radius"/>
    </declare-styleable>

    <declare-styleable name="ShapedImageView">
        <attr name="shape_mode" format="enum">
            <enum name="round_rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
        <attr name="round_radius" format="dimension" />
    </declare-styleable>
</resources>